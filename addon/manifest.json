{
  "manifest_version": 2,
  "name": "Tab Image Saver",
  "applications": {
    "gecko": {
      "strict_min_version": "60.0",
      "id": "tab-image-saver@mcdamo.addons.mozilla.org"
    }
  },
  "permissions": [
    "tabs",
    "storage",
    "notifications",
    "downloads",
    "<all_urls>"
  ],
  "icons": {
    "48": "icons/tab-image-saver-v2@48.png"
  },
  "browser_action": {
    "default_icon": {
      "48": "icons/tab-image-saver-v2.svg"
    },
    "theme_icons": [
      {
        "light": "icons/tab-image-saver-v2-light.svg",
        "dark": "icons/tab-image-saver-v2.svg",
        "size": 48
      }
    ],
    "default_title": "Tab Image Saver",
    "browser_style": true
  },
  "options_ui": {
    "page": "options.html",
    "browser_style": true
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {},
      "description": "Run Tab Image Saver with current settings"
    }
  },
  "version": "2.0.4",
  "homepage_url": "https://github.com/mcdamo/tab-image-saver"
}